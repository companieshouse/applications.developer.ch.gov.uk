{% if this_notifications is not null %}

  {% for notification in this_notifications %}
    <div aria-labelledby="notification-banner-title" role="alert" tabindex="-1" data-module="initial-focus">
        <h2 class="govuk-heading-m" id="notification-banner-title" 
        style="margin-top: 1em; border:5px; border-style:solid; border-color:#00703c; padding: 1em;">{{ notification }}</h2>
    </div>
  {% endfor %}
{% endif %}